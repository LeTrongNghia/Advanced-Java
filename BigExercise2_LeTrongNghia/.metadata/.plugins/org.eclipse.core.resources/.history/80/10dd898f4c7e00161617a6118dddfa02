 * @version 1.0
 * @description this is main class
 */
public class ManagementCD {
	public static void main(String[] args) throws ClassNotFoundException, SQLException {
		// TODO Auto-generated method stub
		CDContoller cdController = new CDContoller();
		List<CD> list = cdController.getListCD();
		list.forEach(i -> System.out.println(i.toString()));
		CD cd = new CD("Mot nha", "Lê Trọng Nghĩa", 2, 10000);
		if (!cdController.addCD(cd))
			System.out.println("Insert susccess");
		else
			System.out.println("Failed");
	}

}
